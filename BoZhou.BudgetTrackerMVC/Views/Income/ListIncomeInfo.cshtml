@model List<BoZhou.BudgetTracker.ApplicationCore.Models.IncomeResponseModel>
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Income Id</th>
                <th scope="col">User Id</th>
                <th scope="col">Amount</th>
                <th scope="col">Description</th>
                <th scope="col">Remarks</th>
                <th scope="col">Income Date</th>
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var res in Model)
            {
            <tr>
                <td>@res.Id</td>
                <td>@res.UserId</td>
                <td>@res.Amount</td>
                <td>@res.Description</td>
                <td>@res.Remarks</td>
                <td>@res.IncomeDate</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Income" asp-action="DeleteIncomeInTable" asp-route-id="@res.Id">Delete</a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>
